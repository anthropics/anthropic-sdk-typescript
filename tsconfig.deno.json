{
  "extends": "./tsconfig.json",
  "include": ["deno"],
  "exclude": [],
  "compilerOptions": {
    "rootDir": "./deno",
    "lib": ["es2020", "DOM"],
    "paths": {
      "@anthropic-ai/sdk/*": ["deno/*"],
      "@anthropic-ai/sdk": ["deno/index.ts"]
    },
    "noEmit": true,
    "declaration": true,
    "declarationMap": true,
    "outDir": "deno",
    "pretty": true,
    "sourceMap": true
  }
}
